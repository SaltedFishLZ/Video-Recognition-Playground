{
    "test_transforms": [
        {
            "package": "transforms",
            "name": "CenterSegment",
            "argv": {
                "size": 16
            }
        },
        {
            "name": "Resize",
            "argv": {
                "size": 256
            }
        },
        {
            "name": "CenterCrop",
            "argv": {
                "size": [224, 224]
            }
        },
        {
            "package": "torchstream.transforms",
            "name": "ToTensor"
        },
        {
            "package": "torchstream.transforms",
            "name": "Normalize" ,
            "argv": {
                "mean": [0.485, 0.456, 0.406],
                "std": [0.229, 0.224, 0.225]
            }
        }
    ],

    "test_dataset": {
        "name": "SomethingSomethingV1",
        "argv": {
            "train": false
        }
    },

    "test_loader":{
        "batch_size": 2,
        "num_workers": 32,
        "shuffle": false,
        "pin_memory": true,
        "drop_last": false
    },


    "discriminator": {
        "package": "models",
        "name": "FrameQualityDiscriminator",
        "argv": {
            "input_size": [16, 224, 224]
        }
    },

    "train": {
        "epochs": 100,
        "backup": {
            "dir_path": "checkpoints/discriminator",
            "pth_name": "discriminator.s8.f16.scratch.lr5e-4.e100"
        },
        "resume": {
            "dir_path": "checkpoints/discriminator",
            "pth_name": "discriminator.s8.f16.scratch.lr5e-4.e100"
        }
    }

}
