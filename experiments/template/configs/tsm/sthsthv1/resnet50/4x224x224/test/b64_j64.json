{
    "test_transforms": [
        {
            "package": "torchstream.transforms",
            "name": "CenterSegment",
            "argv": {
                "size": 4
            }      
        },
        {
            "name": "Resize",
            "argv": {
                "size": 256
            }      
        },
        {
            "name": "CenterCrop",
            "argv": {
                "size": [224, 224]
            }      
        },
        {
            "package": "torchstream.transforms",
            "name": "ToTensor"  
        },
        {
            "package": "torchstream.transforms",
            "name": "Normalize" ,
            "argv": {
                "mean": [0.485, 0.456, 0.406],
                "std": [0.229, 0.224, 0.225]
            }
        }
    ],

    "test_dataset": {
        "name": "SomethingSomethingV1",
        "argv": {
            "root": "~/Datasets/Sth-sth/Sth-sth-v1-jpg",
            "train": false
        }
    },

    "test_loader":{
        "batch_size": 64,
        "num_workers": 64,
        "shuffle": false,
        "pin_memory": true,
        "drop_last": false
    },

    "model": {
        "package": "torchstream.models",
        "name": "TSM",
        "argv": {
            "cls_num": 174,
            "input_size": [4, 224, 224],
            "base_model": "resnet50",
            "dropout": 0.8,
            "partial_bn": true,
            "use_softmax": false
        }
    },

    "criterion": {
        "name": "CrossEntropyLoss"
    }

}
